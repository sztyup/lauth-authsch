{
  "name": "sztyup/lauth-authsch",
  "description": "Auth.sch plugin for LAuth",
  "license": "LGPL-3.0-only",
  "authors": [{
    "name": "Csupity László",
    "email": "sztyup@gmail.com"
  }],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "sztyup/lauth": "^0.4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "~4.0",
    "mockery/mockery": "^1.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Sztyup\\LAuth\\Authsch\\": "src/Authsch"
    }
